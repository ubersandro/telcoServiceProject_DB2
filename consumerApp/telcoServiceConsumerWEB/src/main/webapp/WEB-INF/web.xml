<?xml version="1.0" encoding="UTF-8"?>
<web-app 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" 
version="4.0">
  <display-name>telcoServiceConsumerWEB</display-name>
  <filter>
  	<filter-name>LoginChecker</filter-name>
  	<filter-class>filters.LoggedIn</filter-class>
  </filter>

	<filter-mapping>
		    <filter-name>LoginChecker</filter-name> 
						
			<url-pattern>/EmployeeHome</url-pattern>  
			<url-pattern>/CreateServicePackage</url-pattern>  
			<url-pattern>/CreateOptionalProduct</url-pattern> 
			<url-pattern>/SalesReport</url-pattern> 
			 
	</filter-mapping>
</web-app>

